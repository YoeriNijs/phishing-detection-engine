{
  "manifest_version": 2,
  "name": "Phishing Prevention Plugin",
  "version": "1.0",
  "description": "A phishing prevention plugin for Firefox.",
//  "permissions": ["activeTab"],
//  "background": {
//    "scripts": ["background.js"],
//    "persistent": false
//  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["./dist/browsers/firefox/firefox_plugin.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}

