{
  "manifest_version": 2,
  "name": "Phishing Detection Plugin",
  "version": "0.1",
  "description": "Blocks possible phishing requests",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["plugin.js"]
  },
  "icons": {
    "48": "icon.png"
  }
}